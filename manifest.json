{
    "manifest_version": 3,
    "name": "SQLite Wasm Demo Extension",
    "version": "0.0.1",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
    },
    "description": "Testing SQLite WASM Demo in a Chrome Extension",
    "cross_origin_embedder_policy": {
        "value": "require-corp"
    },
    "cross_origin_opener_policy": {
        "value": "same-origin"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
}
